{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format cjs --dts",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "db:schema:create": "tsx src/scripts/schema-create.ts",
    "db:schema:update": "tsx src/scripts/schema-update.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.2.0",
    "@mikro-orm/core": "^6.6.7",
    "@mikro-orm/postgresql": "^6.6.7",
    "bcrypt": "^6.0.0",
    "fastify": "^5.7.4",
    "jose": "^6.1.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@seoul-advanture/eslint-config": "workspace:*",
    "@seoul-advanture/prettier-config": "workspace:*",
    "@seoul-advanture/typescript-config": "workspace:*",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.19.35",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
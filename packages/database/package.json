{
  "name": "@seoul-advanture/database",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:migrate": "mikro-orm migration:up",
    "db:migrate:create": "mikro-orm migration:create",
    "db:seed": "tsx src/seed.ts",
    "db:schema:fresh": "mikro-orm schema:fresh --run"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.6.7",
    "@mikro-orm/migrations": "^6.6.7",
    "@mikro-orm/postgresql": "^6.6.7",
    "uuid": "^13.0.0",
    "bcryptjs": "^3.0.3"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.6.7",
    "@seoul-advanture/typescript-config": "workspace:*",
    "@types/uuid": "^11.0.0",
    "@types/bcryptjs": "^3.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  }
}